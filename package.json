    "@types/isomorphic-fetch": "0.0.34",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.7",
    "@types/web3": "^1.0.12",
    "@xyo-network/api-originchain-graphql": "^0.1.98",

    "apollo-link-http": "^1.5.5",
    "apollo-server": "^2.2.2",
    "async": "^2.6.1",
    "cids": "^0.5.5",
    "commander": "^2.19.0",
    "cross-fetch": "^2.2.3",
    "dotenv": "^6.1.0",
    "dotenv-expand": "^4.2.0",
    "dotenv-extended": "^2.3.0",
    "firebase-admin": "^6.1.0",
    "graphql": "^14.0.2",
    "graphql-import": "^0.7.1",
    "graphql-tag": "^2.10.0",
    "graphql-tools": "^4.0.3",
    "ipfs": "^0.33.1",
    "ipfs-api": "^26.1.2",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.11",
